{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["!.next/cache/**", ".next/**", "dist/**"]
    },
    "docs": {
      "dependsOn": ["^dev"],
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    }
  }
}
